{"_id":"l8mRuUzkPNOGJ0ea","name":"Ava 6","type":"synthetic","img":"modules/alienrpg-starterset/images/avatar/ava-6-avatar.png","data":{"header":{"health":{"value":5,"label":"Health","max":5},"npc":false,"synthstress":false},"attributes":{"str":{"value":5,"label":"Strength","proficient":0,"mod":5},"wit":{"value":5,"label":"Wits","mod":5},"agl":{"value":6,"label":"Agility","mod":6},"emp":{"value":4,"label":"Empathy","mod":4}},"skills":{"heavyMach":{"value":0,"label":"Heavy Machinery","ability":"str","description":"Heavy Machinery","mod":5},"closeCbt":{"value":0,"label":"Close Combat","ability":"str","description":"Close Combat","mod":5},"stamina":{"value":2,"label":"Stamina","ability":"str","description":"Stamina","mod":7},"rangedCbt":{"value":0,"label":"Ranged Combat","ability":"agl","description":"Ranged Combat","mod":6},"mobility":{"value":1,"label":"Mobility","ability":"agl","description":"Mobility","mod":7},"piloting":{"value":0,"label":"Piloting","ability":"agl","description":"Piloting","mod":6},"command":{"value":0,"label":"Command","ability":"wit","description":"Command","mod":5},"manipulation":{"value":0,"label":"Manipulation","ability":"wit","description":"Manipulation","mod":5},"medicalAid":{"value":2,"label":"Medical Aid","ability":"wit","description":"Medical Aid","mod":7},"observation":{"value":2,"label":"Observation","ability":"emp","description":"Observation","mod":6},"survival":{"value":0,"label":"Survival","ability":"emp","description":"Survival","mod":4},"comtech":{"value":3,"label":"Comtech","ability":"emp","description":"Comtech","mod":7}},"general":{"career":{"value":"9","label":"career"},"appearance":{"value":"Android of the USCSS Cronus\nAge: Appears to be in her twenties\nPersonality: Helpful","label":"appearance"},"sigItem":{"value":"","label":"sigItem"},"agenda":{"value":"help the humans on the Cronus survive, without regard to your own safety.","label":"agenda"},"relOne":{"value":"","label":"relOne"},"relTwo":{"value":"","label":"relTwo"},"