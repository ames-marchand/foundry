{"_id":"RppS2Lli3sMb5Iwz","name":"Seegson Microview-2000SE","type":"item","img":"modules/alienrpg-starterset/images/icons/backpack.svg","data":{"header":{"type":{"value":"3","label":"Text"},"active":false},"attributes":{"weight":{"value":0,"label":"Number"},"cost":{"value":"US$25,000.00","label":"Number"},"comment":{"value":"Location tracking.\n\nA navigational map system used on space stations, the Microview 2000SE uses the device’s coordinates aboard the station to determine that \"You are Here\" at all times-wherever here is at the time.","label":"Text"},"food":{"value":0,"label":"Number"},"water":{"value":0,"label":"Number"},"power":{"value":0,"label":"Stress"},"airsupply":{"value":0,"label":"Number"},"quantity":{"value":1},"notes":{"notes":""}},"modifiers":{"attributes":{"health":{"value":0,"label":"Health"},"stress":{"value":0,"label":"Stress"},"str":{"value":0,"label":"Strength"},"wit":{"value":0,"label":"Wits"},"agl":{"value":0,"label":"Agility"},"emp":{"value":0,"label":"Empathy"}},"skills":{"heavyMach":{"value":0,"label":"Heavy Machinery","ability":"str"},"closeCbt":{"value":0,"label":"Close Combat","ability":"str"},"stamina":{"value":0,"label":"Stamina","ability":"str"},"rangedCbt":{"value":0,"label":"Ranged Combat","ability":"agl"},"mobility":{"value":0,"label":"Mobility","ability":"agl"},"piloting":{"value":0,"label":"Piloting","ability":"agl"},"command":{"value":0,"label":"Command","ability":"emp"},"manipulation":{"value":0,"label":"Manipulation","ability":"emp"},"medicalAid":{"value":0,"label":"Medical Aid","ability":"emp"},"observation":{"value":0,"label":"Observation","ability":"wit"},"survival":{"value":0,"label":"Survival","ability":"wit"},"comtech":{"value":0,"label":"Comtech","ability":"wit"}}}},"effects":[],"folder":"suPlGSZ648d3sChX","sort":0,"permission":{"default":0,"YEoPmwinvi5w1FW6":3,"3SlHgCFcwdscgES2":3,"rS4UGcVJp4e6YrUz":3},"flags":{"core":{"sourceId":"Item.JMboq0uJh3CvNIv6"}}}
{"_id":"RtHgfQmzgZanNYLO","name":"The Long Haul","type":"talent","img":"modules/alienrpg-corerules/images/icons/fire-dash.svg","data":{"attributes":{"flag":{"value":0,"label":"Flag"}},"general":{"career":{"value":"10","label":"career"},"comment":{"value":"You've seen it and done it all before. Nothing surprises you anymore. Once per Act in Cinematic play and once per game session in Campaign play, you may ignore all Failure's from a single roll.","label":"Text"}},"notes":{"notes":""}},"effects":[],"folder":"sYOuAZYTn0HGo9HG","sort":0,"permission":{"default":0,"023AT5ERR8Ij00Wy":3,"M2aZuGY0mCdDk004":3,"rS4UGcVJp4e6YrUz":3},"flags":{"core":{"sourceId":"Compendium.alienrpg-corerules.alienrpg-talents-career.OtMlbO9gaU7JxoId"}}}
